<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Cadastro de Benefici√°rios</display-name>
  <servlet>
    <servlet-name>
      ListarBeneficiarios
    </servlet-name>
    <servlet-class>
      br.gov.incra.beneficiario.servlet.ListarBeneficiariosServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
      ListarBeneficiarios
    </servlet-name>
    <url-pattern>
      /
    </url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>
      IniciarCadastro
    </servlet-name>
    <servlet-class>
      br.gov.incra.beneficiario.servlet.IniciarCadastroServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
      IniciarCadastro
    </servlet-name>
    <url-pattern>
      /iniciar-cadastro
    </url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>
      RealizarCadastro
    </servlet-name>
    <servlet-class>
      br.gov.incra.beneficiarios.servlet.RealizarCadastroServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
     RealizarCadastro
    </servlet-name>
    <url-pattern>
      /realizar-cadastro
    </url-pattern>
  </servlet-mapping>
</web-app>
